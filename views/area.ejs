    data.forEach(element => {
        let content = `
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">
                    <p id="parkingName" class="fw-bold">${element.name}</p>
                </h5>
                <p class="card-text">
                    <p id="parkingType" class="badge text-bg-success">Type: ${element.type}</p>
