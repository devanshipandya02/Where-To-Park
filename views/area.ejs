                <p class="card-text">
                    <p id="opening_time">Time: <em class="muted">${element.opening_time}</em></p>
                </p>
                <button data-map="${element.iframe}&output=embed" target="blank" class="btn btn-primary" id="${element.id}" onclick="getMap(this.id)">Open Map</button>
                <button type="button" class="btn btn-danger" data-bs-toggle="modal" data-bs-target="#exampleModal">
             Pay Now
                </button>
            </div>
        `
        document.getElementById('parking').innerHTML += content;

    });

let getMap = (id)=> {
